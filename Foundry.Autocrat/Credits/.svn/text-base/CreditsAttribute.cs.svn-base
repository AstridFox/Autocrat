using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Foundry.Autocrat.Credits {
	[global::System.AttributeUsage(AttributeTargets.Assembly, Inherited = false, AllowMultiple = true)]
	public sealed class CreditsAttribute : Attribute {
		public CreditsAttribute(string credits) {
			Credits = credits;
		}

		public string Credits { get; set; }
	}
}
